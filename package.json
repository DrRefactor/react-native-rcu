{
  "name": "ReactNativeRCU",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "yarn reset-cache && react-native start --reset-cache",
    "reset-cache": "rm -rf /tmp/metro-* && watchman watch-del-all",
    "test": "jest",
    "lint": "eslint .",
    "yalc-reinstall": "yarn yalc-add && yarn",
    "yalc-add": "yalc add react-native-focus-kit",
    "--yalc-add-pure": "yalc add react-native-focus-kit --pure",
    "--yalc-link": "yalc link react-native-focus-kit",
    "--yalc-update": "yalc update"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "npm:react-native-tvos@0.63.1-1",
    "react-native-focus-kit": "git+https://drrefactor@github.com/drrefactor/react-native-focus-kit"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "0.63.1",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
